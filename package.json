{
  "name": "@popeindustries/lit-html-server",
  "version": "0.12.0",
  "description": "Render lit-html templates on the server",
  "keywords": [
    "html template",
    "lit-html",
    "stream",
    "streaming",
    "tagged template literal",
    "template"
  ],
  "main": "index.js",
  "repository": "https://github.com/popeindustries/lit-html-server.git",
  "author": "Alexander Pope <alex@pope-industries.com>",
  "license": "MIT",
  "dependencies": {
    "get-stream": "^4.0.0",
    "he": "^1.1.1",
    "readable-stream": "^3.0.2"
  },
  "devDependencies": {
    "autocannon": "^2.4.1",
    "babel-eslint": "^9.0.0",
    "chai": "^4.1.2",
    "eslint": "^5.5.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "lit-html": "^0.11.0",
    "mocha": "^5.2.0",
    "prettier": "^1.14.2"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "lint": "eslint './{lib,test}/**/*.js'",
    "perf": "node test/server.js & PID=$!; autocannon -c 100 -d 5 -p 10 http://localhost:3000 && kill $PID",
    "precommit": "lint-staged",
    "test": "NODE_ENV=test mocha \"test/*-test.js\" --reporter spec --bail --timeout 2000"
  }
}
