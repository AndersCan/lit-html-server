{
  "name": "@popeindustries/lit-html-server",
  "version": "0.14.4",
  "description": "Render lit-html templates on the server",
  "keywords": [
    "html template",
    "lit-html",
    "stream",
    "streaming",
    "tagged template literal",
    "template"
  ],
  "main": "index.js",
  "browser": "browser.js",
  "repository": "https://github.com/popeindustries/lit-html-server.git",
  "author": "Alexander Pope <alex@pope-industries.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "autocannon": "^3.2.0",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-prettier": "^3.0.1",
    "esm": "^3.1.0",
    "get-stream": "^4.1.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "lit-html": "^1.0.0-rc.2",
    "mocha": "^5.2.0",
    "prettier": "^1.16.0",
    "rollup": "^1.1.1",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "format": "prettier --write './{directives,lib,test}/**/*.{js,json}'",
    "lint": "eslint './{directives,lib,test}/**/*.js'",
    "perf": "node test/server.js & PID=$!; autocannon -c 100 -d 5 -p 10 http://localhost:3000 && kill $PID",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run build",
    "test": "NODE_ENV=test mocha \"test/*-test.js\" --reporter spec --bail --timeout 2000 --require esm"
  }
}
